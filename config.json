{
    "name": "Generate Alternate Text Vision",
    "version": "v0.0.0",
    "actions": [
        {
            "title": "Set Alternate Description  (Vision AI)",
            "name": "pdfix_generate_alternate_text_vision",
            "desc": "Automatically generates alternate text for Figure tags using Vision, embedding it into each figure's Alt attribute [Local]",
            "version": "v0.0.0",
            "icon": "add_photo_alternate_vision",
            "category": "Tags",
            "local": "True",
            "program": "docker run -v \"${working_directory}:/data\" -w /data --rm pdfix/generate-alternate-text-vision:latest generate-alt-text -i \"${input_pdf}\" -o \"/data/${output_pdf}\" --overwrite \"${overwrite}\" --zoom \"${zoom}\" --name \"${license_name}\" --key \"${license_key}\" --model /model",
            "args": [
                {
                    "name": "input_pdf",
                    "desc": "Input PDF file",
                    "flags": 2,
                    "type": "file_path",
                    "ext": "pdf",
                    "value": ""
                },
                {
                    "name": "output_pdf",
                    "desc": "Output PDF file.",
                    "flags": 4,
                    "type": "file_path",
                    "ext": "pdf",
                    "value": ""
                },
                {
                    "title": "Overwrite",
                    "desc": "Replace the alternate text if it exists",
                    "name": "overwrite",
                    "type": "bool",
                    "value": false
                },
                {
                    "title": "Rendering Zoom Level",
                    "name": "zoom",
                    "desc": "Pages are provided as images that are rendered using this zoom level",
                    "type": "float",
                    "value": "2.0"
                }
            ],
            "errors": {
                "10": "Failed to parse arguments. Please check the usage and try again.",
                "11": "Input file does not exists.",
                "12": "Not allowed input output file combination. Please see --help",
                "20": "Failed to initialize PDFix SDK.",
                "21": "Failed to activate PDFix SDK acount.",
                "22": "Failed to authorize PDFix SDK acount.",
                "23": "Failed to render PDF Page into image.",
                "24": "Failed to open PDF document.",
                "25": "Failed to save PDF document.",
                "26": "PDF document has no tags."
            }
        },
        {
            "title": "Set Alternate Description (Vision AI)",
            "name": "pdfix_generate_alternate_text_from_image_to_text_vision",
            "desc": "Automatically generates alternate text from an image file using Vision, saving the description to a text file [Local]",
            "version": "v0.0.0",
            "icon": "add_photo_alternate_vision",
            "category": "Tags",
            "subtype": "generate_alt_text_img_to_txt",
            "local": "True",
            "program": "docker run -v \"${working_directory}:/data\" -w /data --rm pdfix/generate-alternate-text-vision:latest generate-alt-text -i \"${input_jpg}\" -o \"/data/${output_txt}\" --model /model",
            "args": [
                {
                    "name": "input_jpg",
                    "desc": "Input image file containing picture",
                    "flags": 2,
                    "type": "file_path",
                    "ext": "jpg",
                    "value": ""
                },
                {
                    "name": "output_txt",
                    "desc": "Output TXT file containing alt text",
                    "flags": 4,
                    "type": "file_path",
                    "ext": "txt",
                    "value": ""
                }
            ],
            "errors": {
                "10": "Failed to parse arguments. Please check the usage and try again.",
                "11": "Input file does not exists.",
                "12": "Not allowed input output file combination. Please see --help",
                "20": "Failed to initialize PDFix SDK.",
                "21": "Failed to activate PDFix SDK acount.",
                "22": "Failed to authorize PDFix SDK acount.",
                "23": "Failed to render PDF Page into image.",
                "24": "Failed to open PDF document.",
                "25": "Failed to save PDF document.",
                "26": "PDF document has no tags."
            }
        }
    ]
}
